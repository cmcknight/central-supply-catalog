{
  "name": "central-supply-catalog",
  "version": "1.0.0",
  "description": "The Central Supply Catalog (CSC) provides an online catalog of equipment and supplies for the Cepheus Engine and Traveller Science Fiction Role-Playing Games (SFRPG). The catalog is a living resource and allows new entries to be submitted via pull requests. The CESC is hosted on GitHub Pages and is a free resource for the community.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "ELEVENTY_ENV=dev npx @11ty/eleventy --output=build --pathprefix=; sass src/scss:build/css",
    "prod": "ELEVENTY_ENV=prod npx @11ty/eleventy --output=dist; sass src/scss:dist/css",
    "sass": "sass src/scss:build/css",
    "gen-search-index": "node ./src/util/search.js  ",
    "watch:sass": "npm run sass -- --watch",
    "watch:eleventy": "npx @11ty/eleventy --serve --output=build",
    "start-no-sass": "ELEVENTY_ENV=dev npm-run-all sass --parallel watch:*",
    "start": "ELEVENTY_ENV=dev npx eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmcknight/central-supply-catalog.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cmcknight/central-supply-catalog/issues"
  },
  "homepage": "https://github.com/cmcknight/central-supply-catalog#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "babel-cli": "^6.26.0",
    "browser-sync": "^2.27.5",
    "browserlist": "^1.0.1",
    "cheerio": "^1.0.0-rc.9",
    "chokidar": "^3.5.1",
    "core-js": "^2.6.12",
    "cssnano": "^5.0.5",
    "del": "^6.0.0",
    "fsevents": "^2.3.2",
    "gifsicle": "^5.2.0",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-copy": "^4.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-remove-empty-lines": "^0.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-save": "^1.3.0",
    "gulp-sitemap": "^8.0.0",
    "gulp-uglify": "^3.0.2",
    "imagemin-mozjpeg": "^9.0.0",
    "minisearch": "^3.1.0",
    "moment": "^2.29.1",
    "mozjpeg": "^6.0.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "optipng": "^3.0.0",
    "readdirp": "^3.6.0",
    "sass": "^1.39.2",
    "svgo": "^2.3.0",
    "uglify": "^0.1.5"
  },
  "devDependencies": {
    "gulp": "^4.0.2"
  }
}
